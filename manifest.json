{
    "name": "HistorySE",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "http://www.historyse.com chrome client",
    "icons": {
      "128": "assets/images/icon128.png",
      "48": "assets/images/icon48.png",
      "16": "assets/images/icon16.png"
    },
    "browser_action": {
        "default_icon": "assets/images/icon128.png",
        "default_title": "HistorySE",
        "default_popup": "sign_in.html"
    },
    "background": {
        "page": "background.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["assets/javascript/public/jquery.min.js", "assets/javascript/window_events_notifier.js"]
      }
    ],
    "permissions": [
        "webRequest",
        "http://*/*",
        "https://*/*"
    ]
}
